<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextView.Poppins.Regular" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">15sp</item>

    </style>

    <style name="EditText.Poppins.Corner.Background" parent="Widget.AppCompat.EditText">
        <item name="android:background">@drawable/bg_edit_text</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:textColorHint">@color/text_color</item>
        <item name="android:padding">@dimen/dimen_padding_20</item>

    </style>

    <style name="TextView.Poppins.Bold" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/poppins_semi_bold</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/orange_text_color</item>

    </style>


    <style name="foodRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingstars</item>
        <item name="android:minHeight">10dp</item>
        <item name="android:maxHeight">10dp</item>
    </style>

</resources>